    "@iconify-json/ep": "^1.1.6",
    "@types/node": "^18.0.3",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@unocss/preset-attributify": "^0.44.7",
